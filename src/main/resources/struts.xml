<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="default" namespace="/" extends="struts-default">

		<action name="showxml" class="com.xmlui.actions.XmlViewer" method="showXML">
			<result name="SUCCESS">viewxml.jsp</result>
		</action>
		
		<action name="savexml" class="com.xmlui.actions.XmlViewer" method="saveXML">
			<result name="SUCCESS">pages/viewxml.jsp</result>
		</action>
		
		
	</package>

</struts>